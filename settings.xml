<!-- Maven Configuration -->

<settings>
  <servers>
    <server>
      <id>oss.sonatype.org</id>
      <username>${env.OSSRH_USER}</username>
      <password>${env.OSSRH_PASS}</password>
	</server>
  </servers>
  <profiles>
    <profile>
      <id>spoofzu/DeepViolet</id>
      <properties>
        <gpg.keyname>${env.GPG_KEY_NAME}</gpg.keyname>
        <gpg.passphrase>${env.GPG_PASSPHRASE}</gpg.passphrase>
      </properties>
    </profile>
  </profiles>
</settings>
